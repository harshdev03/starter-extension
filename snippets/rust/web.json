{
  "Actix Web Basic Server": {
    "prefix": "actix-server",
    "body": [
      "use actix_web::{get, App, HttpServer, Responder};",
      "",
      "#[get(\"/\")]",
      "async fn hello() -> impl Responder {",
      "    \"Hello, world!\"",
      "}",
      "",
      "#[actix_web::main]",
      "async fn main() -> std::io::Result<()> {",
      "    HttpServer::new(|| {",
      "        App::new().service(hello)",
      "    })",
      "    .bind((\"127.0.0.1\", 8080))?",
      "    .run()",
      "    .await",
      "}"
    ],
    "description": "Basic Actix Web server with one route"
  },

  "Actix Web POST Endpoint": {
    "prefix": "actix-post",
    "body": [
      "use actix_web::{post, web, App, HttpServer, Responder};",
      "",
      "#[post(\"/submit\")]",
      "async fn submit(data: web::Json<serde_json::Value>) -> impl Responder {",
      "    format!(\"Received: {}\", data)",
      "}",
      "",
      "#[actix_web::main]",
      "async fn main() -> std::io::Result<()> {",
      "    HttpServer::new(|| {",
      "        App::new().service(submit)",
      "    })",
      "    .bind((\"127.0.0.1\", 8080))?",
      "    .run()",
      "    .await",
      "}"
    ],
    "description": "POST endpoint example in Actix-web"
  }
}
