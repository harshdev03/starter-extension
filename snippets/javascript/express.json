{
  "Express App": {
    "prefix": "js-express",
    "body": [
      "const express = require('express');",
      "const app = express();",
      "const PORT = process.env.PORT || 3000;",
      "",
      "app.get('/', (req, res) => {",
      "  res.send('Hello Express');",
      "});",
      "",
      "app.listen(PORT, () => console.log(`Server running on port ${PORT}`));"
    ],
    "description": "Express.js server boilerplate"
  }
}
